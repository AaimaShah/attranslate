{
  "name": "aktranslate",
  "version": "0.0.1",
  "description": "Integrate automated translation services into your app development. Convert between file formats.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fkirc/aktranslate.git"
  },
  "keywords": [
    "google-translate",
    "translate",
    "mobile app translation",
    "cross-platform translation",
    "i18n",
    "react-i18next"
  ],
  "license": "MIT",
  "author": "Felix Kirchengast",
  "bugs": {
    "url": "https://github.com/fkirc/aktranslate/issues"
  },
  "homepage": "https://github.com/fkirc/aktranslate#readme",
  "bin": {
    "aktranslate": "bin/aktranslate"
  },
  "main": "./dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "lint": "npm run eslint",
    "reformat": "npm run eslint -- --fix",
    "eslint": "./node_modules/.bin/eslint '**/*.{js,ts,tsx}' --ignore-path=.gitignore",
    "test": "jest --runInBand --maxConcurrency=1",
    "prepublishOnly": "git diff --exit-code"
  },
  "husky": {
    "hooks": {
      "pre-commit": "exit 0 && TODO"
    }
  },
  "dependencies": {
    "@google-cloud/translate": "^2.1.3",
    "chalk": "^2.4.2",
    "commander": "^6.1.0",
    "deep-object-diff": "^1.1.0",
    "dotenv": "^6.2.0",
    "inquirer": "^6.2.2",
    "lodash": "^4.17.11",
    "messageformat-parser": "^4.1.0",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "ts-node": "^8.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "rimraf": "^3.0.2",
    "node-fetch": "^2.3.0",
    "@types/jest": "^26.0.13",
    "@types/inquirer": "^0.0.44",
    "@types/dotenv": "^6.1.0",
    "@types/ncp": "^2.0.3",
    "@types/lodash": "^4.14.122",
    "@types/node-fetch": "^2.1.6",
    "@types/node": "^14.10.1",
    "typescript": "^4.0.2",
    "eslint": "^7.8.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "prettier": "^2.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
